.zz{
  width:100%;
  display:flex;
  flex-direction: column;
  justify-content:center;
  align-items: center;
}
.add-wrap {
  z-index: 100;
  position:fixed;
  bottom:210rpx;
  right:30rpx;
  display:flex;
  width:50%;
  justify-content:flex-end;
}

.icon-item-name{
  font-size: 10px;
  color:#024FFD;
}

.icon-item{
  display: block;
  padding: 16px 0;
  width: 33.33%;
  float: left;
  text-align: center;
}


button::after {
  border: none;
  padding: 0;
  margin: 0;
}

button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: inherit;
}

